{
  "functions": {
    "api/v1/rework/oauth/start.js": {
      "includeFiles": "server/**"
    },
    "api/v1/rework/oauth/callback.js": {
      "includeFiles": "server/**"
    },
    "api/v1/rework/oauth/handoff.js": {
      "includeFiles": "server/**"
    },
    "api/v1/rework/oauth/refresh.js": {
      "includeFiles": "server/**"
    }
  },
  "headers": [
    {
      "source": "/api/v1/rework/oauth/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ]
}
